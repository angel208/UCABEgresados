<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>UCABEgresados</title>

    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/styles.css" rel="stylesheet">
    <link href="/css/simple-sidebar.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto|Roboto+Condensed" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>


    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>


<body>
<!--!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!      SIDE BAR                !!!!!!!!!!!!!!!!!!!!11 -->
<div id="wrapper">

    <!-- Titulo en el sidebar -->
    <div id="sidebar-wrapper">
        <%include Partials/Sidebar_Egresados.ejs%>



    </div>
<!--!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!      PAGE CONTENT               !!!!!!!!!!!!!!!!!!!!11 -->

    <!-- Page Content -->
    <div id="page-content-wrapper" class="vertical-center-parent main-background-light" >
        <div class="container-fluid " >

                    <h3 style="color: black;" class="main-background-title"> Resultados de las Elecciones.</h3>
                    


<br>
<br>
     <div class="container-fluid">
  
  

  

  <% cargos.forEach( function( cargos ) {%>
 <div class="panel panel-primary">
          
    <div class="panel-heading">

     <h4> <strong><%= cargos.NombreCargo %> </strong></h4>
      </div>

        <div class="panel-body">
            
            <table class="table">
                
            <thead>
                 <tr>
                   <th>Postulado</th>
                   <th>Votos</th>
                 </tr>
            </thead>
    <tbody>
        <% GANADOR.forEach( function( GANA ) {%>
                 <% if (cargos.IDCargo1 == GANA.IDCargo){%>
      <tr>
        <td>  <%=GANA.nombre%> </td>
        <td>  <%= GANA.TotalVotos %></td>
        
      </tr>
              <% } %>
                                            <%});%>
        
</tbody>
     
  </table>




        </div>
    </div>
    <%});%>








   </div>


        </div>
    </div>
    <!-- /#page-content-wrapper -->

</div>






<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="/js/jquery.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="/js/bootstrap.min.js"></script>
</body>
</html>