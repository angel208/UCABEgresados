<div class="container-fluid background-login">

    <div class="row" style="height: 100%">

        <!-- VENTANA DE LOGIN -->
        <div class="col-md-4 vcenter-custom-width-parent" style="height: 100%;" >

            <h1> Sistema de Votacion para Egresados </h1>

            <form class="vertical-center-child three-quarters-width" action="/login" method="POST">
                <div class="form-group">
                    <label for="user">Nombre de Usuario:</label>
                    <input type="text" name="user" class="form-control" id="user">
                </div>
                <div class="form-group">
                    <label for="pwd">Contraseña:</label>
                    <input type="password" name="pass" class="form-control" id="pwd">
                </div>
                <a href="#">He olvidado mi contraseña</a>
                <button type="submit" class="btn btn-default pull-right">Submit</button>
            </form>
        </div>
